<?xml version="1.0" encoding="UTF-8"?>
 
<schema version="1.0" xmlns="http://www.apple.com/metadata"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      xsi:schemaLocation="http://www.apple.com/metadata file:///System/Library/Frameworks/CoreServices.framework/Frameworks/Metadata.framework/Resources/MetadataSchema.xsd">
    <note>
        Custom attributes that this metadata importer supports.  Below
	is an example of a multivalued string attribute.  Other types
	are CFNumber, CFDate, CFBoolean and CFData.		      
    </note>
    <attributes>
        <attribute name="org_sidmusic_Released" type="CFString"/>
        <attribute name="org_sidmusic_DefaultSubtune" type="CFNumber"/>
        <attribute name="org_sidmusic_SubtuneCount" type="CFNumber"/>
    </attributes>

    <types>
        <type name="org.sidmusic.sidtune">
            <allattrs>
				kMDItemTitle
				kMDItemComposer
				org_sidmusic_Released
				org_sidmusic_DefaultSubtune
				org_sidmusic_SubtuneCount
            </allattrs>
            <displayattrs>
				kMDItemTitle
				kMDItemComposer
				org_sidmusic_Released
				org_sidmusic_DefaultSubtune
				org_sidmusic_SubtuneCount
            </displayattrs>
        </type>
    </types>
</schema>

